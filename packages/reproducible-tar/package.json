{
  "name": "reproducible-tar",
  "version": "1.0.0",
  "description": "extract, transform and re-pack tarball entries in form of stream",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "build": "yarn run clean && yarn run build:lib && yarn run build:types",
    "build:lib": "rollup -c",
    "build:types": "tsc -p tsconfig.dts.json --outDir dist --declaration --emitDeclarationOnly",
    "clean": "rimraf dist",
    "ensure-linted": "eslint --max-warnings 0 . --ext=js,ts",
    "lint:fix": "eslint --cache --max-warnings 0 --fix",
    "precommit": "lint-staged",
    "semantic-release": "semantic-release",
    "test": "jest"
  },
  "dependencies": {
    "@types/tar-stream": "^2.1.0",
    "core-js": "^3.6.5",
    "gunzip-maybe": "^1.4.1",
    "tar-stream": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/register": "7.12.1",
    "@rollup/plugin-typescript": "4.1.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@types/jest": "25.2.3",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "builtin-modules": "3.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "jest": "25.5.4",
    "lint-staged": "10.5.1",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "rollup": "2.33.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-generate-package-json": "3.2.0",
    "rollup-plugin-node-externals": "2.2.0",
    "semantic-release": "17.2.2",
    "ts-jest": "25.5.1",
    "typescript": "3.9.7"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "duplex",
    "gzip",
    "tar",
    "tarball",
    "tgz",
    "transform"
  ],
  "license": "MIT",
  "runkitExampleFilename": "runkit.js",
  "types": "dist/index.d.ts"
}
